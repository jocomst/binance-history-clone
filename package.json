{
  "name": "binance-historic",
  "author": "maxgfr",
  "license": "MIT",
  "description": "Get historical klines from binance api",
  "repository": {
    "type": "git",
    "url": "https://github.com/maxgfr/binance-historic.git"
  },
  "keywords": [
    "node",
    "typescript",
    "binance",
    "historic"
  ],
  "scripts": {
    "start": "node build/main.js",
    "dev": "nodemon",
    "develop": "swc ./src -d build && node build/main.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "clean": "rimraf build",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -w -p tsconfig.json",
    "build:swc": "swc ./src -d build",
    "build:swc:watch": "npm run build:swc -- -w",
    "lint": "eslint ./src --ext .ts",
    "prettier": "prettier --write './src/**/*.{ts,js,json}'"
  },
  "dependencies": {
    "axios": "^0.26.0",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@swc/cli": "0.1.55",
    "@swc/core": "1.2.150",
    "@swc/jest": "0.2.20",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.26",
    "@types/prompts": "^2.0.14",
    "@typescript-eslint/eslint-plugin": "5.13.0",
    "@typescript-eslint/parser": "5.13.0",
    "eslint": "8.10.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jest": "26.1.1",
    "jest": "27.5.1",
    "nodemon": "2.0.15",
    "prettier": "2.5.1",
    "rimraf": "3.0.2",
    "ts-jest": "27.1.3",
    "ts-node": "10.7.0",
    "typescript": "4.6.2"
  },
  "release": {
    "branches": [
      "main",
      "next"
    ]
  }
}
